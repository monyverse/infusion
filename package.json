{
  "name": "uniteai-wallet",
  "version": "1.0.0",
  "description": "AI-Powered Cross-Chain Wallet Automation with 1inch Fusion+ Integration",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\" \"npm run dev:ai\"",
    "dev:frontend": "next dev",
    "dev:backend": "PORT=3003 ts-node src/backend/server.ts",
    "dev:ai": "node src/ai/simple-agent-manager.js",
    "build": "next build",
    "start": "next start",
    "compile": "hardhat compile",
    "deploy": "hardhat run scripts/deploy.ts --network sepolia",
    "test": "jest",
    "test:ai": "jest --testPathPattern=ai",
    "test:contracts": "jest --testPathPattern=contracts",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write .",
    "ai:train": "ts-node src/ai/training/train-agents.ts",
    "ai:simulate": "ts-node src/ai/simulation/simulate-trades.ts",
    "deploy:testnets": "hardhat run scripts/deploy-all-testnets.ts",
    "verify:testnets": "hardhat run scripts/verify-all-contracts.ts",
    "deploy:sepolia": "hardhat run scripts/deploy.ts --network sepolia",
    "deploy:arbitrum-sepolia": "hardhat run scripts/deploy.ts --network arbitrumSepolia",
    "deploy:polygon-mumbai": "hardhat run scripts/deploy.ts --network polygonMumbai",
    "deploy:base-sepolia": "hardhat run scripts/deploy.ts --network baseSepolia",
    "deploy:optimism-sepolia": "hardhat run scripts/deploy.ts --network optimismSepolia",
    "deploy:bsc-testnet": "hardhat run scripts/deploy.ts --network bscTestnet",
    "deploy:avalanche-fuji": "hardhat run scripts/deploy.ts --network avalancheFuji",
    "deploy:fantom-testnet": "hardhat run scripts/deploy.ts --network fantomTestnet",
    "deploy:etherlink": "hardhat run scripts/deploy.ts --network etherlink",
    "verify:sepolia": "hardhat verify --network sepolia",
    "verify:arbitrum-sepolia": "hardhat verify --network arbitrumSepolia",
    "verify:polygon-mumbai": "hardhat verify --network polygonMumbai",
    "verify:base-sepolia": "hardhat verify --network baseSepolia",
    "verify:optimism-sepolia": "hardhat verify --network optimismSepolia",
    "verify:bsc-testnet": "hardhat verify --network bscTestnet",
    "verify:avalanche-fuji": "hardhat verify --network avalancheFuji",
    "verify:fantom-testnet": "hardhat verify --network fantomTestnet",
    "verify:etherlink": "hardhat verify --network etherlink",
    "near:build": "cd near-contracts/fusion-escrow && cargo near build non-reproducible-wasm && cd ../fusion-pool && cargo near build non-reproducible-wasm && cd ../fusion-solver && cargo near build non-reproducible-wasm",
    "near:deploy": "cd near-contracts && chmod +x deploy.sh && ./deploy.sh",
    "near:deploy:build-only": "cd near-contracts && chmod +x deploy.sh && ./deploy.sh --build-only",
    "near:setup": "./scripts/setup-near.sh setup",
    "near:create-account": "./scripts/setup-near.sh create-account",
    "near:check-balance": "./scripts/setup-near.sh check-balance",
    "near:full-setup": "./scripts/setup-near.sh full-setup",
    "near:deploy-final": "cd near-contracts && ./deploy-final.sh",
    "near:initialize": "cd near-contracts && ./initialize-contracts.sh",
    "near:test": "cd near-contracts && chmod +x deploy.sh && ./deploy.sh --test-only",
    "backend:start": "node src/backend/simple-server.js",
    "backend:dev": "nodemon src/backend/simple-server.js",
    "ai:start": "node src/ai/simple-agent-manager.js",
    "ai:dev": "nodemon src/ai/simple-agent-manager.js",
    "test:integration": "jest --testPathPattern=integration",
    "test:near": "jest --testPathPattern=near",
    "test:btc": "jest --testPathPattern=btc",
    "test:evm": "jest --testPathPattern=evm",
    "test:aptos": "jest --testPathPattern=aptos",
    "test:sui": "jest --testPathPattern=sui",
    "test:bitcoin": "jest --testPathPattern=bitcoin",
    "test:connectivity": "jest --testPathPattern=connectivity",
    "deploy:contracts": "ts-node scripts/deploy-contracts.ts",
    "deploy:ethereum": "ts-node scripts/deploy-contracts.ts --network ethereum",
    "deploy:polygon": "ts-node scripts/deploy-contracts.ts --network polygon",
    "deploy:aptos": "ts-node scripts/deploy-contracts.ts --network aptos",
    "deploy:sui": "ts-node scripts/deploy-contracts.ts --network sui",
    "deploy:bitcoin-style": "ts-node scripts/deploy-contracts.ts --network bitcoin-style",
    "verify:contracts": "ts-node scripts/verify-contracts.ts",
    "validate:state": "ts-node scripts/validate-state.ts",
    "compile:aptos": "aptos move compile --package-dir ./src/contracts/aptos",
    "compile:sui": "sui move build --package-path ./src/contracts/sui",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next out dist build coverage",
    "reset": "npm run clean && npm install && npm run build",
    "setup": "chmod +x scripts/setup-dev.sh && ./scripts/setup-dev.sh",
    "setup:env": "chmod +x scripts/setup-dev.sh && ./scripts/setup-dev.sh --env-only",
    "setup:quick": "chmod +x scripts/setup-dev.sh && ./scripts/setup-dev.sh --skip-tests --skip-lint"
  },
  "keywords": [
    "ai-wallet",
    "cross-chain",
    "defi",
    "1inch",
    "fusion-plus",
    "bitcoin",
    "near",
    "stellar",
    "etherlink",
    "atomic-swap",
    "ai-automation",
    "intent-based",
    "limit-orders"
  ],
  "author": "UniteAI Wallet Team",
  "license": "MIT",
