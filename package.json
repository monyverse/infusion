{
  "name": "uniteai-wallet",
  "version": "1.0.0",
  "description": "AI-Powered Cross-Chain Wallet Automation with 1inch Fusion+ Integration",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\" \"npm run dev:ai\"",
    "dev:frontend": "next dev",
    "dev:backend": "node src/backend/simple-server.js",
    "dev:ai": "node src/ai/simple-agent-manager.js",
    "build": "next build",
    "start": "next start",
    "compile": "hardhat compile",
    "deploy": "hardhat run scripts/deploy.ts --network sepolia",
    "test": "jest",
    "test:ai": "jest --testPathPattern=ai",
    "test:contracts": "jest --testPathPattern=contracts",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write .",
    "ai:train": "ts-node src/ai/training/train-agents.ts",
    "ai:simulate": "ts-node src/ai/simulation/simulate-trades.ts",
    "fusion:deploy": "hardhat run scripts/deploy-fusion-plus.ts --network sepolia",
    "near:deploy": "hardhat run scripts/deploy-near-solver.ts --network near",
    "stellar:deploy": "hardhat run scripts/deploy-stellar-bridge.ts --network stellar",
    "etherlink:deploy": "hardhat run scripts/deploy-etherlink.ts --network etherlink"
  },
  "keywords": [
    "ai-wallet",
    "cross-chain",
    "defi",
    "1inch",
    "fusion-plus",
    "bitcoin",
    "near",
    "stellar",
    "etherlink",
    "atomic-swap",
    "ai-automation",
    "intent-based",
    "limit-orders"
  ],
  "author": "UniteAI Wallet Team",
  "license": "MIT",
  "dependencies": {
    "@1inch/limit-order-protocol": "^2.0.5",
    "@1inch/limit-order-settlement": "^3.1.0",
    "@1inch/solidity-utils": "^6.6.0",
    "@headlessui/react": "^1.7.0",
    "@heroicons/react": "^2.0.0",
    "@langchain/openai": "^0.6.3",
    "@openzeppelin/contracts": "^5.4.0",
    "@stellar/stellar-sdk": "^13.3.0",
    "@tanstack/react-query": "^5.17.0",
    "autoprefixer": "^10.4.0",
    "axios": "^1.11.0",
    "bcryptjs": "^2.4.3",
    "bip32": "^4.0.0",
    "bip39": "^3.1.0",
    "bitcoinjs-lib": "^6.1.3",
    "bull": "^4.12.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.0",
    "ecpair": "^2.1.0",
    "ethers": "^6.15.0",
    "express": "^4.18.0",
    "express-rate-limit": "^8.0.1",
    "framer-motion": "^10.16.0",
    "helmet": "^7.1.0",
    "immer": "^10.0.0",
    "jsonwebtoken": "^9.0.0",
    "langchain": "^0.3.30",
    "lucide-react": "^0.294.0",
    "multer": "^2.0.2",
    "near-api-js": "^6.2.4",
    "next": "^14.0.0",
    "node-cron": "^3.0.3",
    "openai": "^4.28.0",
    "postcss": "^8.4.0",
    "rate-limiter-flexible": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.47.0",
    "recharts": "^2.8.0",
    "redis": "^4.6.0",
    "socket.io": "^4.7.0",
    "socket.io-client": "^4.7.0",
    "tailwindcss": "^3.3.0",
    "tiny-secp256k1": "^2.2.3",
    "uuid": "^9.0.0",
    "viem": "^2.33.1",
    "wagmi": "^2.16.0",
    "winston": "^3.11.0",
    "ws": "^8.14.0",
    "zod": "^3.22.0",
    "zod-to-json-schema": "^3.22.0",
    "zustand": "^4.4.0",
    "@radix-ui/react-dialog": "^1.1.0",
    "@radix-ui/react-dropdown-menu": "^2.1.0",
    "@radix-ui/react-progress": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.0",
    "@radix-ui/react-toast": "^1.2.0",
    "@radix-ui/react-tooltip": "^1.1.0",
    "tailwind-merge": "^2.2.0",
    "tailwindcss-animate": "^1.0.7",
    "three": "^0.162.0",
    "@react-three/fiber": "^8.15.0",
    "@react-three/drei": "^9.99.0",
    "lottie-react": "^2.4.0",
    "tsparticles": "^3.0.0",
    "react-tsparticles": "^2.12.0",
    "@solana/web3.js": "^1.90.0",
    "@solana/spl-token": "^0.4.0"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^4.0.0",
    "@types/bcryptjs": "^2.4.0",
    "@types/cors": "^2.8.0",
    "@types/express": "^4.17.0",
    "@types/jest": "^29.5.0",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.0.0",
    "@types/node-cron": "^3.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "concurrently": "^8.2.0",
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.0.0",
    "hardhat": "^2.26.1",
    "jest": "^29.7.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.8.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/test/**/*.test.ts",
      "**/src/**/*.test.ts"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/setup.ts"
    ]
  }
}
