# 1inch API Configuration
INCH_API_KEY=your_1inch_api_key_here
INCH_API_URL=https://api.1inch.dev
INCH_FUSION_PLUS_ENABLED=true

# AI Services
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here
COHERE_API_KEY=your_cohere_api_key_here

# Blockchain RPC URLs
ETHEREUM_RPC_URL=https://eth-mainnet.g.alchemy.com/v2/your_key
SEPOLIA_RPC_URL=https://sepolia.drpc.org
ARBITRUM_RPC_URL=https://arb1.arbitrum.io/rpc
POLYGON_RPC_URL=https://polygon-rpc.com
BASE_RPC_URL=https://mainnet.base.org
ETHERLINK_RPC_URL=https://node.ghostnet.etherlink.com

# Bitcoin Configuration
BITCOIN_RPC_URL=http://localhost:8332
BITCOIN_RPC_USER=your_bitcoin_rpc_user
BITCOIN_RPC_PASS=your_bitcoin_rpc_password
BITCOIN_NETWORK=mainnet

# NEAR Configuration
NEAR_RPC_URL=https://rpc.mainnet.near.org
NEAR_ACCOUNT_ID=your_near_account_id
NEAR_PRIVATE_KEY=your_near_private_key

# Stellar Configuration
STELLAR_RPC_URL=https://horizon.stellar.org
STELLAR_NETWORK=public
STELLAR_SECRET_KEY=your_stellar_secret_key

# Database Configuration
REDIS_URL=redis://localhost:6379
DATABASE_URL=postgresql://user:password@localhost:5432/uniteai

# Security & Authentication
JWT_SECRET=your_jwt_secret_key_here
ENCRYPTION_KEY=your_32_character_encryption_key
SESSION_SECRET=your_session_secret_here

# Private Keys (for development only - use hardware wallets in production)
PRIVATE_KEY=your_ethereum_private_key_here
BITCOIN_PRIVATE_KEY=your_bitcoin_private_key_here

# API Keys for External Services
ETHERSCAN_API_KEY=your_etherscan_api_key
ARBISCAN_API_KEY=your_arbiscan_api_key
POLYGONSCAN_API_KEY=your_polygonscan_api_key
BASESCAN_API_KEY=your_basescan_api_key
COINMARKETCAP_API_KEY=your_coinmarketcap_api_key

# Monitoring & Analytics
SENTRY_DSN=your_sentry_dsn_here
MIXPANEL_TOKEN=your_mixpanel_token_here

# Feature Flags
ENABLE_AI_AGENTS=true
ENABLE_CROSS_CHAIN_SWAPS=true
ENABLE_LIMIT_ORDERS=true
ENABLE_PORTFOLIO_REBALANCING=true
ENABLE_RISK_MANAGEMENT=true

# Performance Configuration
AI_RESPONSE_TIMEOUT=30000
SWAP_TIMEOUT=300000
GAS_OPTIMIZATION_ENABLED=true
MAX_SLIPPAGE=0.5

# Development Configuration
NODE_ENV=development
LOG_LEVEL=debug
ENABLE_MOCK_AI=false
ENABLE_TESTNET_ONLY=false 